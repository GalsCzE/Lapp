<Page x:Class="Lapp.NUser"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Lapp"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="NUser">

    <Grid>
        <ScrollViewer>
            <StackPanel>
                <Button x:Name="back_btn" Click="back_btn_Click" Content="Zpět"></Button>
                <Label>Jméno</Label>
                <TextBox x:Name="name" TextChanged="name_TextChanged"></TextBox>
                <Button x:Name="created" Click="created_Click">Vytvořit uživatele</Button>
            </StackPanel>
        </ScrollViewer>
    </Grid>
</Page>
